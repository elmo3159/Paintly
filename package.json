{
  "name": "paintly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3005 -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern='__tests__/(api|components|lib)'",
    "test:integration": "jest --testPathPattern='__tests__/integration'",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest --coverage --testPathPattern='__tests__/(api|components|lib)'",
    "test:coverage:integration": "jest --coverage --testPathPattern='__tests__/integration'",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:open": "npm run test:coverage && open-cli coverage/lcov-report/index.html",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "test:quality": "npm run lint && npm run type-check && npm run test:coverage",
    "test:performance": "playwright test --grep='パフォーマンステスト'",
    "test:accessibility": "playwright test --grep='アクセシビリティテスト'",
    "test:security": "playwright test --grep='セキュリティ'",
    "coverage:merge": "nyc merge coverage coverage/merged.json",
    "coverage:report": "nyc report --reporter=html --reporter=text --reporter=lcov",
    "coverage:check": "nyc check-coverage --lines 85 --functions 85 --branches 80 --statements 85",
    "coverage:badges": "coverage-badges-cli --output coverage/badges",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps"
  },
  "dependencies": {
