    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.469.0",
    "next": "^15.1.4",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-compare-slider": "^2.2.0",
